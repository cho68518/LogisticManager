{
  "version": "1.0",
  "description": "엑셀 컬럼과 데이터베이스 컬럼 간의 매핑 설정",
  "mappings": {
    "order_table": {
      "table_name": "orders",
      "description": "주문 정보 테이블 매핑",
      "columns": {
        "주문번호": {
          "db_column": "order_number",
          "data_type": "varchar",
          "required": true,
          "description": "주문번호"
        },
        "주문일자": {
          "db_column": "order_date",
          "data_type": "date",
          "required": true,
          "description": "주문일자"
        },
        "수취인명": {
          "db_column": "recipient_name",
          "data_type": "varchar",
          "required": true,
          "description": "수취인명"
        },
        "수취인연락처": {
          "db_column": "recipient_phone",
          "data_type": "varchar",
          "required": false,
          "description": "수취인연락처"
        },
        "주소": {
          "db_column": "address",
          "data_type": "varchar",
          "required": true,
          "description": "주소"
        },
        "상세주소": {
          "db_column": "detail_address",
          "data_type": "varchar",
          "required": false,
          "description": "상세주소"
        },
        "우편번호": {
          "db_column": "zip_code",
          "data_type": "varchar",
          "required": false,
          "description": "우편번호"
        },
        "품목코드": {
          "db_column": "product_code",
          "data_type": "varchar",
          "required": false,
          "description": "품목코드"
        },
        "품목명": {
          "db_column": "product_name",
          "data_type": "varchar",
          "required": true,
          "description": "품목명"
        },
        "수량": {
          "db_column": "quantity",
          "data_type": "int",
          "required": true,
          "description": "수량"
        },
        "단가": {
          "db_column": "unit_price",
          "data_type": "decimal",
          "required": false,
          "description": "단가"
        },
        "총액": {
          "db_column": "total_price",
          "data_type": "decimal",
          "required": false,
          "description": "총액"
        },
        "배송타입": {
          "db_column": "shipping_type",
          "data_type": "varchar",
          "required": false,
          "description": "배송타입"
        },
        "출고지": {
          "db_column": "shipping_center",
          "data_type": "varchar",
          "required": true,
          "description": "출고지"
        },
        "결제방법": {
          "db_column": "payment_method",
          "data_type": "varchar",
          "required": false,
          "description": "결제방법"
        },
        "배송비": {
          "db_column": "shipping_cost",
          "data_type": "decimal",
          "required": false,
          "description": "배송비"
        },
        "박스크기": {
          "db_column": "box_size",
          "data_type": "varchar",
          "required": false,
          "description": "박스크기"
        },
        "특이사항": {
          "db_column": "special_note",
          "data_type": "varchar",
          "required": false,
          "description": "특이사항"
        },
        "처리상태": {
          "db_column": "processing_status",
          "data_type": "varchar",
          "required": false,
          "description": "처리상태"
        },
        "매장명": {
          "db_column": "store_name",
          "data_type": "varchar",
          "required": false,
          "description": "매장명"
        },
        "이벤트타입": {
          "db_column": "event_type",
          "data_type": "varchar",
          "required": false,
          "description": "이벤트타입"
        },
        "가격카테고리": {
          "db_column": "price_category",
          "data_type": "varchar",
          "required": false,
          "description": "가격카테고리"
        },
        "지역": {
          "db_column": "region",
          "data_type": "varchar",
          "required": false,
          "description": "지역"
        },
        "배송지역": {
          "db_column": "delivery_area",
          "data_type": "varchar",
          "required": false,
          "description": "배송지역"
        }
      }
    }
  },
  "validation_rules": {
    "required_fields": ["주문번호", "수취인명", "주소", "품목명", "수량", "출고지"],
    "numeric_fields": ["수량", "단가", "총액", "배송비"],
    "date_fields": ["주문일자"]
  },
  "data_transformations": {
    "주소": {
      "special_handling": "star_processing",
      "description": "별표 처리 (감천, 카카오 등 특수 출고지)"
    },
    "수량": {
      "data_type_conversion": "int",
      "default_value": 0
    },
    "단가": {
      "data_type_conversion": "decimal",
      "default_value": 0
    },
    "총액": {
      "data_type_conversion": "decimal",
      "default_value": 0
    },
    "배송비": {
      "data_type_conversion": "decimal",
      "default_value": 0
    }
  }
} 